<?xml version="1.0" encoding="UTF-8"?>
<Environment>
  <Population initialSize="50">
    <NullAgentFactory />
    <MeanSimpleFitnessAggregator />
    <VectorIndividualFactory genomeSize="20">
      <FlatIntegerInitializer ceiling="10" end="19" floor="0" start="0" />
    </VectorIndividualFactory>
    <VectorMutationPipeline>
      <WeightedBreedingPipeline>
        <ElitismSelector proportionElites="0.3" weight="0.5" />
        <TournamentSelector topIndividuals="1" touramentSize="2" weight="4.5" />
        <FitnessProportionalSelector weight="2.0" />
        <RandomIndividualSelector weight="1.0" />
      </WeightedBreedingPipeline>
      <GaussianIntegerMutator end="19" mutationProbability="0.05" sd="5.0" start="0" />
      <IntegerLimiter ceiling="20" end="19" floor="0" start="0" />
    </VectorMutationPipeline>
  </Population>

  <!-- Need an EvaluationGroupFactory -->
  <ShuffledEvaluationGroupFactory groupSize="11" timesThrough="2" />

  <!-- Need an Agent Model Factory -->
  <DefaultAgentModelFactory modelClass="neutrality.NeutralityModel" />

  <!-- Need an Evaluator -->
  <LocalEvaluator />

  <!-- Test out an AgentModelReporter -->
  <AgentModelReporter perStepFile="psf.tsv" summaryFile="sum.tsv" />

</Environment>
